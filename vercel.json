{
  "version": 2,
  "builds": [
    {
      "src": "api/run.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/run",
      "destination": "/api/run"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
